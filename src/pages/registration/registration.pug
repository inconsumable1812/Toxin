extends ../../layout/pages.pug

include ../../components/image-slider/image-slider.pug
include ../../components/registration-form/registration-form.pug

block variables
  - let pageClass = 'registration'

block main
  .registration__slider
    +image-slider({isInterval:true,rows:[
        {src: require('./img/registration_1.png').default, alt: '1'},
        {src: require('./img/registration_2.png').default, alt: '2'},
        ]})
  .registration__form 
    +registration-form
